{
  "name": "terraform",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "inits": "terraform init -upgrade=true src",
    "plan": "terraform plan -var-file envs/terraform.tfvars src",
    "app": "terraform apply -var-file envs/terraform.tfvars -auto-approve src",
    "destroy": "terraform destroy -var-file envs/terraform.tfvars src",
    "destroy:target": "terraform destroy -target=module.E001 -var-file envs/terraform.tfvars src"
  },
  "dependencies": {
    "@types/node": "^12.0.0",
    "aws-sdk": "^2.449.0"
  }
}
